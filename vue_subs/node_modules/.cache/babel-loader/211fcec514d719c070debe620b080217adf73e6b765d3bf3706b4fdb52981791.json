{"ast":null,"code":"import Layout from \"@/layout/c-main.vue\";\nimport { Swiper, SwiperSlide } from 'swiper/vue';\nimport { Navigation, Autoplay } from 'swiper/modules';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nexport default {\n  components: {\n    Layout,\n    Swiper,\n    SwiperSlide\n  },\n  setup() {\n    return {\n      modules: [Navigation, Autoplay]\n    };\n  }\n};","map":{"version":3,"names":["Layout","Swiper","SwiperSlide","Navigation","Autoplay","components","setup","modules"],"sources":["/home/josh/Documents/projects/subs/vue_subs/src/views/components/advance/slider.vue"],"sourcesContent":["<script>\r\nimport Layout from \"@/layout/c-main.vue\"\r\nimport { Swiper, SwiperSlide } from 'swiper/vue';\r\n\r\nimport { Navigation, Autoplay } from 'swiper/modules';\r\n\r\n// Import Swiper styles\r\nimport 'swiper/css';\r\nimport 'swiper/css/navigation';\r\n\r\n\r\nexport default {\r\n    components: {\r\n        Layout,\r\n        Swiper,\r\n        SwiperSlide,\r\n    },\r\n    setup() {\r\n        return {\r\n            modules: [Navigation, Autoplay],\r\n        };\r\n    },\r\n}\r\n</script>\r\n\r\n<template>\r\n    <Layout>\r\n        <BRow>\r\n            <BCol md=\"10\" xxl=\"9\" class=\"mb-4\">\r\n                <h2>Slider</h2>\r\n                <p class=\"text-muted\">tiny-slider works with static content and it works in the browser only. If the HTML is\r\n                    loaded dynamically.</p>\r\n                <div>\r\n                    <a class=\"btn btn-sm btn-light-dark rounded-pill px-2\" role=\"button\" target=\"_blank\"\r\n                        href=\"http://ganlanyuan.github.io/tiny-slider/demo/\">\r\n                        <i class=\"ti ti-external-link me-1\"></i>\r\n                        Reference\r\n                    </a>\r\n                </div>\r\n            </BCol>\r\n        </BRow>\r\n\r\n        <BRow>\r\n            <BCol md=\"6\">\r\n                <BCard no-body>\r\n 