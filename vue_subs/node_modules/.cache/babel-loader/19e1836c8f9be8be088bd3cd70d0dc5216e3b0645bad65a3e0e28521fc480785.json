{"ast":null,"code":"import Layout from \"@/layout/main.vue\";\nimport pageheader from \"@/components/page-header.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"PLANS\",\n  components: {\n    Layout,\n    pageheader\n  },\n  data() {\n    return {\n      modalShow: false,\n      plans: [],\n      plan: {\n        id: \"\",\n        plan_name: \"\",\n        plan_description: \"\",\n        plan_up: \"\",\n        plan_down: \"\",\n        price: \"\"\n      },\n      operation: false\n    };\n  },\n  created() {\n    this.getPlans();\n  },\n  methods: {\n    updatePlan() {\n      if (this.operation == false) {\n        axios.post(\"/api/v1/plans/\", {\n          plan_name: this.plan.plan_name,\n          plan_description: this.plan.plan_description,\n          plan_up: this.plan.plan_up,\n          plan_down: this.plan.plan_down,\n          price: this.plan.price\n        }).then(response => {\n          console.log(response.data);\n          this.modalShow = !this.modalShow;\n          this.getPlans();\n        });\n      } else {\n        axios.path(`/api/v1/plans/${this.plan.id}/`, {\n          plan_name: this.plan.plan_name,\n          plan_description: this.plan.plan_description,\n          plan_up: this.plan.plan_up,\n          plan_down: this.plan.plan_down,\n          price: this.plan.price\n        }).then(response => {\n          console.log(response.data);\n          this.modalShow = !this.modalShow;\n          this.getPlans();\n        });\n      }\n    },\n    getPlans() {\n      axios.get(\"/api/v1/plans/\").then(response => {\n        this.plans = response.data;\n      });\n    }\n  }\n};","map":{"version":3,"names":["Layout","pageheader","axios","name","components","data","modalShow","plans","plan","id","plan_name","plan_description","plan_up","plan_down","price","operation","created","getPlans","methods","updatePlan","post","then","response","console","log","path","get"],"sources":["/home/josh/Documents/projects/subs/vue_subs/src/views/adashboard/subscription-plans.vue"],"sourcesContent":["<script>\nimport Layout from \"@/layout/main.vue\"\nimport pageheader from \"@/components/page-header.vue\"\nimport axios from \"axios\";\n\nexport default {\n    name: \"PLANS\",\n    components: {\n        Layout, pageheader\n    },\n    data() {\n        return {\n            modalShow: false,\n            plans: [],\n            plan: {\n                id:\"\",\n                plan_name: \"\",\n                plan_description: \"\",\n                plan_up: \"\",\n                plan_down: \"\",\n                price: \"\",\n            },\n            operation: false,\n        }\n    },\n    created() {\n        this.getPlans();\n    },\n    methods: {\n        updatePlan() {\n            if (this.operation==false) {\n                axios.post(\"/api/v1/plans/\", { \n                    plan_name: this.plan.plan_name,\n                    plan_description: this.plan.plan_description,\n                    plan_up: this.plan.plan_up,\n                    plan_down: this.plan.plan_down,\n                    price: this.plan.price,\n                }).then((response) => {\n                    console.log(response.data);\n                    this.modalShow = !this.modalShow;\n                    this.getPlans();\n                });\n            } else {\n                axios.path(`/api/v1/plans/${this.plan.id}/`, { \n                    plan_name: this.plan.plan_name,\n                    plan_description: this.plan.plan_description,\n                    plan_up: this.plan.plan_up,\n                    plan_down: this.plan.plan_down,\n                    price: this.plan.price,\n                }).then((response) => {\n                    console.log(response.data);\n                    this.modalShow = !this.modalShow;\n                    this.getPlans();\n                });\n            }\n        },\n        getPlans() {\n            axios.get(\"/api/v1/plans/\").then((response)=> {\n                this.plans = response.data;\n            });\n        }\n    }\n}\n</script>\n\n<template>\n    <Layout>\n        <pageheader title=\"Subscription Plan list\" pageTitle=\"Plans\" />\n        <BRow>\n            <BCol class=\"col-sm-12\">\n                <BCard no-body class=\"table-card\">\n                    <BCardBody>\n                        <div class=\"text-end p-sm-4 pb-sm-2\">\n                            <!-- <router-link to=\"/add-product\" class=\"btn btn-primary\"> <i class=\"ti ti-plus f-18\"></i> Add Region </router-link>-->\n                            <BButton variant=\"primary\" @click=\"modalShow = !modalShow\">Add Plan</BButton>\n                        </div>\n                        <div class=\"table-responsive\">\n                            <table class=\"table table-hover tbl-product\" id=\"pc-dt-simple\">\n                                <thead>\n                                    <tr>\n                                        <th class=\"text-end\">#</th>\n                                        <th>Plan Detail</th>\n                                        <th>Bandwidth (up/down)</th>\n                                        <th class=\"text-end\">Price</th>\n                                    </tr>\n                                </thead>\n                                <tbody>\n                                    <tr v-for=\"(plan, index) in plans\" :key=\"plan.id\">\n                                        <td class=\"text-end\">{{index + 1}}</td>\n                                        <td>\n                                            <BRow class=\"d-flex\">\n                                                <BCol>\n                                                    <h6 class=\"mb-1\">{{ plan.plan_name }}</h6>\n                                                    <p class=\"text-muted f-12 mb-0\">{{ plan.plan_description }} </p>\n                                                </BCol>\n                                            </BRow>\n                                        </td>\n                                        <td>{{ plan.plan_down }}/{{ plan.plan_up }}</td>\n                                        <td class=\"text-end\">{{ plan.price }}</td>\n                                        <td class=\"text-center\">\n                                            <div class=\"prod-action-links\">\n                                                <ul class=\"list-inline me-auto mb-0\">\n                                                    <li class=\"list-inline-item align-bottom\" data-bs-toggle=\"tooltip\" title=\"View\">\n                                                        <a href=\"#\" class=\"avtar avtar-xs btn-link-secondary btn-pc-default\" data-bs-toggle=\"offcanvas\" data-bs-target=\"#productOffcanvas\">\n                                                            <i class=\"ti ti-eye f-18\"></i>\n                                                        </a>\n                                                    </li>\n                                                    <li class=\"list-inline-item align-bottom\" data-bs-toggle=\"tooltip\" title=\"Edit\">\n                                                        <router-link to=\"/add-product\" class=\" avtar avtar-xs btn-link-success btn-pc-default\">\n                                                            <i class=\"ti ti-edit-circle f-18\"></i>\n                                                        </router-link>\n                                                    </li>\n                                                    <li class=\"list-inline-item align-bottom\" data-bs-toggle=\"tooltip\" title=\"Delete\">\n                                                        <a href=\"#\" class=\"avtar avtar-xs btn-link-danger btn-pc-default\">\n                                                            <i class=\"ti ti-trash f-18\"></i>\n                                                        </a>\n                                                    </li>\n                                                </ul>\n                                            </div>\n                                        </td>\n                                    </tr>\n                                    \n                                </tbody>\n                            </table>\n                        </div>\n                    </BCardBody>\n                </BCard>\n            </BCol>\n        </BRow>\n\n        <BRow>\n            <BCol sm=\"12\">\n                <BCardBody class=\"pc-component\">\n                    <div>\n                        \n                        <BModal v-model=\"modalShow\" title=\"Subscription Plan\" hide-footer class=\"v-modal-custom\" size=\"lg\">\n                            <div class=\"modal-body  p-0\">\n                                <BRow class=\" mb-3\">\n                                    <label for=\"plan\" class=\"col-sm-2 col-form-label\">Plan name</label>\n                                    <BCol class=\"col-sm-10\">\n                                        <input v-model=\"plan.plan_name\" type=\"text\" class=\"form-control\" id=\"plan\">\n                                    </BCol>\n                                </BRow>\n                                <BRow class=\" mb-3\">\n                                    <label for=\"description\" class=\"col-sm-2 col-form-label\">Description</label>\n                                    <BCol class=\"col-sm-10\">\n                                        <textarea v-model=\"plan.plan_description\" class=\"form-control\" id=\"description\" rows=\"3\"></textarea>\n                                    </BCol>\n                                </BRow>\n                                <BRow class=\" mb-3\">\n                                    <label for=\"down\" class=\"col-sm-2 col-form-label\">Download</label>\n                                    <BCol class=\"col-sm-4\">\n                                        <div class=\"input-group mb-3\">\n                                            <input v-model=\"plan.plan_down\" type=\"text\" class=\"form-control\" aria-label=\"Amount (to the nearest peso)\">\n                                            <!-- <span class=\"input-group-text\">.00</span> -->\n                                        </div>\n                                    </BCol>\n                                    <label for=\"up\" class=\"col-sm-2 col-form-label\">Upload</label>\n                                    <BCol class=\"col-sm-4\">\n                                        <div class=\"input-group mb-3\">\n                                            <input v-model=\"plan.plan_up\" type=\"text\" class=\"form-control\" aria-label=\"Amount (to the nearest peso)\">\n                                            <!-- <span class=\"input-group-text\">.00</span> -->\n                                        </div>\n                                    </BCol>\n                                </BRow>\n                                <BRow class=\" mb-3\">\n                                    <label for=\"price\" class=\"col-sm-2 col-form-label\">Price</label>\n                                    <BCol class=\"col-sm-4\">\n                                        <div class=\"input-group mb-3\">\n                                            <span class=\"input-group-text\">Php</span>\n                                            <input v-model=\"plan.price\" type=\"text\" class=\"form-control\" aria-label=\"Amount (to the nearest peso)\">\n                                            <span class=\"input-group-text\">.00</span>\n                                        </div>\n                                    </BCol>\n                                </BRow>\n                            </div>\n                            <div class=\"modal-footer pb-0\">\n                                <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\" @click=\"modalShow = !modalShow\">Close</button>\n                                <button type=\"button\" class=\"btn btn-primary\" @click=\"addPlan\">Save changes</button>\n                            </div>\n                        </BModal>\n                    </div>\n\n                </BCardBody>\n            </BCol>\n        </BRow>\n    </Layout>\n</template>"],"mappings":"AACA,OAAOA,MAAK,MAAO,mBAAkB;AACrC,OAAOC,UAAS,MAAO,8BAA6B;AACpD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IACRJ,MAAM;IAAEC;EACZ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,KAAK;MAChBC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;QACFC,EAAE,EAAC,EAAE;QACLC,SAAS,EAAE,EAAE;QACbC,gBAAgB,EAAE,EAAE;QACpBC,OAAO,EAAE,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,KAAK,EAAE;MACX,CAAC;MACDC,SAAS,EAAE;IACf;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,QAAQ,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACT,IAAI,IAAI,CAACJ,SAAS,IAAE,KAAK,EAAE;QACvBb,KAAK,CAACkB,IAAI,CAAC,gBAAgB,EAAE;UACzBV,SAAS,EAAE,IAAI,CAACF,IAAI,CAACE,SAAS;UAC9BC,gBAAgB,EAAE,IAAI,CAACH,IAAI,CAACG,gBAAgB;UAC5CC,OAAO,EAAE,IAAI,CAACJ,IAAI,CAACI,OAAO;UAC1BC,SAAS,EAAE,IAAI,CAACL,IAAI,CAACK,SAAS;UAC9BC,KAAK,EAAE,IAAI,CAACN,IAAI,CAACM;QACrB,CAAC,CAAC,CAACO,IAAI,CAAEC,QAAQ,IAAK;UAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACjB,IAAI,CAAC;UAC1B,IAAI,CAACC,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;UAChC,IAAI,CAACW,QAAQ,CAAC,CAAC;QACnB,CAAC,CAAC;MACN,OAAO;QACHf,KAAK,CAACuB,IAAI,CAAC,iBAAiB,IAAI,CAACjB,IAAI,CAACC,EAAE,GAAG,EAAE;UACzCC,SAAS,EAAE,IAAI,CAACF,IAAI,CAACE,SAAS;UAC9BC,gBAAgB,EAAE,IAAI,CAACH,IAAI,CAACG,gBAAgB;UAC5CC,OAAO,EAAE,IAAI,CAACJ,IAAI,CAACI,OAAO;UAC1BC,SAAS,EAAE,IAAI,CAACL,IAAI,CAACK,SAAS;UAC9BC,KAAK,EAAE,IAAI,CAACN,IAAI,CAACM;QACrB,CAAC,CAAC,CAACO,IAAI,CAAEC,QAAQ,IAAK;UAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACjB,IAAI,CAAC;UAC1B,IAAI,CAACC,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;UAChC,IAAI,CAACW,QAAQ,CAAC,CAAC;QACnB,CAAC,CAAC;MACN;IACJ,CAAC;IACDA,QAAQA,CAAA,EAAG;MACPf,KAAK,CAACwB,GAAG,CAAC,gBAAgB,CAAC,CAACL,IAAI,CAAEC,QAAQ,IAAI;QAC1C,IAA